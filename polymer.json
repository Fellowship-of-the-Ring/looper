{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/page-markets.html",
    "src/page-market.html",
    "src/page-mywallet.html",
    "src/page-404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint":
  {
    "rules": ["polymer-2"]
  },
  "builds": [
  {
    "preset": "es5-bundled"
  },
  {
    "preset": "es6-bundled"
  },
  {
    "preset": "es6-unbundled"
  }]
}